<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.anteater.star.domain.wedding.Guest">
        <!--  主键 -->
        <id name="guest_id" column="guest_id">
            <generator class="native"></generator>
        </id>
        <property name="guest_name"></property>
        <property name="guest_gift"></property>
        <property name="guest_type"></property>
        <property name="guest_table"></property>
        <property name="guest_num"></property>
    </class>
</hibernate-mapping>